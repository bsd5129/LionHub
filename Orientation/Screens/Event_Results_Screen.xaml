<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Orientation.Event_Results_Screen" Title="Event Results Screen">
	<ContentPage.Content>
		<StackLayout >
			<StackLayout VerticalOptions="Start" Padding="15, 30, 15, 0" Spacing="20">
	            <Label Text="Event Name: {Binding data}" FontSize="14"></Label>
		        <Label Text="Description: {Binding data}" FontSize="14"></Label>
		        <Label Text="Date: {Binding data}" FontSize="14"></Label>
		        <Label Text="Time: {Binding data}" FontSize="14"></Label>
		        <Label  Text="Location: {Binding data}" FontSize="14"></Label>
		        <Label Text="Coordinator Name: {Binding data}" FontSize="14"></Label>
		        <Label Text="Coordinator Phone: {Binding data}" FontSize="14"></Label>
		        <Label Text="Misc.Info: {Binding data}" FontSize="14"></Label>
		      
		    </StackLayout>
            <StackLayout Orientation="Horizontal" VerticalOptions="End"></StackLayout>
        </StackLayout>
	
	</ContentPage.Content>
</ContentPage>