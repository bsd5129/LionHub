<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Orientation.Event_Results_Screen" Title="Event Results Screen">
	<ContentPage.Content>
		<StackLayout x:Name="stackLayout">
			<StackLayout VerticalOptions="Start" Padding="15, 30, 15, 0" Spacing="20">
	            <Label x:Name="eventName" Text="Event Name: {Binding data}" FontSize="14"></Label>
		        <Label x:Name="description" Text="Description: {Binding data}" FontSize="14"></Label>
		        <Label x:Name="date" Text="Date: {Binding data}" FontSize="14"></Label>
		        <Label x:Name="time" Text="Time: {Binding data}" FontSize="14"></Label>
		       <Label x:Name="location" Text="Location: {Binding data}" FontSize="14"></Label>
		        <Label x:Name="coordinatorName" Text="Coordinator Name: {Binding data}" FontSize="14"></Label>
		        <Label x:Name="coordinatorPhone" Text="Coordinator Phone: {Binding data}" FontSize="14"></Label>
		        <Label x:Name="info" Text="Misc.Info: {Binding data}" FontSize="14"></Label>
		      
		    </StackLayout>
            <StackLayout x:Name="bottomLayout" Orientation="Horizontal" VerticalOptions="End"></StackLayout>
        </StackLayout>
	
	</ContentPage.Content>
</ContentPage>